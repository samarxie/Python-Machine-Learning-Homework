import math

def is_prime_number(num):
    #判断素数的核心
    for i in range(2, int(math.sqrt(num)) + 1):
        if num % i != 0:
            continue
        else:
            return False

    return True

for i in range(2,30):
    if is_prime_number(i):
        print(i)
    else:
        continue
